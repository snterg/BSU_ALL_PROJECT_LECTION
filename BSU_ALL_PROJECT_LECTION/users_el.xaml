<Window x:Class="BSU_ALL_PROJECT_LECTION.users_el"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BSU_ALL_PROJECT_LECTION"
        mc:Ignorable="d"
        Title="users_el" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <local:simple_el_u Foreground="Aquamarine" />
            <local:clr_picker_BSU  x:Name="clr_slider" ColorChanged="clr_ColorChanged"/>
        </StackPanel>
        <Button Content="Принять" Click="Button_Click" Grid.Column="1"/>
    </Grid>
</Window>
